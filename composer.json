{
    "name": "gustavorattmann/dashboard",
    "description": "Treinamento com PHP puro e Orientação a Objetos",
    "type": "project",
    "require": {
        "vlucas/phpdotenv": "^5.4",
        "twig/twig": "^3.0",
        "doctrine/orm": "^2.6.2",
        "symfony/yaml": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.11",
        "monolog/monolog": "^2.3.5"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "App\\": "app/",
            "Src\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Gustavo Rattmann",
            "email": "gustavo_rattmann@hotmail.com.br"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    }
}
